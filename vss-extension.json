{
    "manifestVersion": 1,
    "id": "kmitton-feature-flag-manager",
    "publisher": "kenneth-mitton",
    "version": "1.1.10",
    "name": "Feature Flag Manager",
    "description": "An extension for managing feature flags.",
    "public": true,
    "categories": [
        "Azure Repos"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "icons": {
        "default": "icon.default.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ktmitton/azure-devops-feature-flag-manager"
    },
    "scopes": [
        "vso.code_write",
        "vso.extension.data_write",
        "vso.graph",
        "vso.identity"
    ],
    "contributions": [
        {
            "id": "feature-flag-hub",
            "type": "ms.vss-web.hub",
            "description": "Adds a feature flag hub",
            "targets": [
                "ms.vss-web.home-hub-group"
            ],
            "properties": {
                "name": "Feature Flags",
                "order": 100,
                "uri": "index.html",
                "iconName": "Flag"
            }
        },
        {
            "id": "feature-flag-settings",
            "type": "ms.vss-web.hub",
            "description": "Manages the settings for the Feature Flags extension",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "Feature Flags Settings",
                "order": 100,
                "uri": "settings.html"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "dist/bundle.js",
            "addressable": true
        },
        {
            "path": "dist/acb0b44d710a15f95219.woff",
            "addressable": true
        },
        {
            "path": "settings.html",
            "addressable": true
        },
        {
            "path": "example-manifest.json",
            "addressable": true
        }
    ]
}