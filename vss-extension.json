{
    "manifestVersion": 1,
    "id": "kmitton-feature-flag-manager",
    "publisher": "kenneth-mitton",
    "version": "1.0.180",
    "name": "Feature Flag Manager",
    "description": "An extension for managing feature flags.",
    "public": false,
    "categories": [
        "Azure Repos"
    ],
    "content": {
        "details": {
            "path": "PACKAGE-DESCRIPTION.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.code_write",
        "vso.extension.data_write",
        "vso.graph",
        "vso.identity"
    ],
    "contributions": [
        {
            "id": "feature-flag-hub-group",
            "type": "ms.vss-web.hub-group",
            "description": "Add a feature flag hub group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Feature Flags",
                "order": 100
            }
        },
        {
            "id": "feature-flag-hub",
            "type": "ms.vss-web.hub",
            "description": "Adds a feature flag hub",
            "targets": [
                ".feature-flag-hub-group"
            ],
            "properties": {
                "name": "Feature Flags",
                "order": 100,
                "uri": "index.html"
            }
        },
        {
            "id": "feature-flag-settings",
            "type": "ms.vss-web.hub",
            "description": "Manages the settings for the Feature Flags extension",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "Feature Flags Settings",
                "order": 100,
                "uri": "settings.html"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "dist/bundle.js",
            "addressable": true
        },
        {
            "path": "dist/acb0b44d710a15f95219.woff",
            "addressable": true
        },
        {
            "path": "settings.html",
            "addressable": true
        }
    ]
}